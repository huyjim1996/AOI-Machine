<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.9">
  <POU Name="PRG_MachineControl" Id="{ae1b1aa1-fabe-4a54-8a0f-85ccaf95cd7c}" SpecialFunc="None">
    <Declaration><![CDATA[(* =======================================================================================================================================
   Automatic Optical Inspection Machine Program
   =======================================================================================================================================
   References
   ======================================================================================================================================= *)
PROGRAM PRG_MachineControl
VAR
	bFirstCycle 		: BOOL := TRUE;			(*  *)
	fbMachineControler 	: FB_MachineControl;	(*  *)
	fbFileLogger 		: FB_FileLogger;		(*	*)
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF bFirstCycle THEN
	bFirstCycle := FALSE;
	
	(* Assign machine controler reference to Shell API*)
	stShell.stPlatform.ref_fbMachineControler REF= fbMachineControler;
	
	(* Set assignamnet done bit for visu controler *)
	stShell.stFlags.bMachineCtrlAssignamentsDone := TRUE;
	
	fbMachineLogger.MR_FirstCycle( 	sName		:= 'AOI_Machine_Logger',
									sFileDir 	:= 'C:\TwinCAT\Loggers',
									sFileName 	:= 'AOI_Machine_Logger',
									sAmsNetId 	:= '',
									bAddTimeStamp := FALSE,
									bAddHeader := FALSE);
END_IF

IF NOT fbMachineLogger.Active THEN 
	fbMachineLogger.MR_Init();	
END_IF
// =========================================================
(* Run global machine clock *)
fbMachineTime(bEnable := TRUE);
(* Cyclic run of machine controler *)
fbMachineControler();
(* Machine Logger *)
fbMachineLogger();
// =========================================================
]]></ST>
    </Implementation>
